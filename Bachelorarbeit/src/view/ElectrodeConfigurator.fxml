                        <TableColumn fx:id="typeCol" prefWidth="75.0" text="Type" />
                        <TableColumn fx:id="zoomCol" prefWidth="75.0" text="Zoom" />
                     </columns>
                  </TableView>
               </children>
            </AnchorPane>
         </center>
         <bottom>
            <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="0.0" prefWidth="600.0" BorderPane.alignment="CENTER">
