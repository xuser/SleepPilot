                        <Button fx:id="btnDeselectAll" mnemonicParsing="false" onAction="#deselectAllAction" text="deselect all" />
                        <Button fx:id="btnInvertSelection" mnemonicParsing="false" onAction="#invertSelectionAction" text="invert selection" />
                     </children>
                  </TilePane>
               </children>
            </AnchorPane>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
